<Window x:Class="PcapGenerator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PcapGenerator"
        mc:Ignorable="d"
        Title="Pcap Generator" Height="286.941" Width="552.47" WindowStartupLocation="CenterScreen">
    <Grid>
    <TabControl TabStripPlacement="Left">
        <TabItem Header="Send">
                <ContentControl Content="{Binding _SendPcapWindow, Mode=OneWay}"/>
            </TabItem>
        <TabItem Header="Change">
                <ContentControl />
            </TabItem>
    </TabControl>
    </Grid>
</Window>
